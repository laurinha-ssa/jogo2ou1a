#include <stdio.h>


int main()
{
  float a,b,c,d,e,f;
    int cont;
        for(cont=0;cont<6;cont++)
        {
         scanf("%f",&a);
            if(a>0)
                {
                  cont++;
                }
                scanf("%f",&b);
            if(b>0)
                {
                    cont++;
                }
                scanf("%f",&c);
            if(c>0)
                {
                  cont++;
                }
                scanf("%f",&d);
            if(d>0)
                {
                   cont++;
                }
                scanf("%f",&e);
            if(e>0)
                {
                   cont++;
                }
                scanf("%f",&f);
            if(f>0)
                {
                   cont++;
                }

                printf("%i valores positivos\n",cont);

        }
        return 0;
    }
