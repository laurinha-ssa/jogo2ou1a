#include <stdio.h>


int main()
{
    int v[5];
    int i,x, encontrei = 0;

    for(i=0;i<5;i++)
    {
      scanf("%i",&v[i]);
    }

    scanf("%i",&x);
    for(i=0;i<5;i++)
    {
        if(v[i]==x)
        {
         printf("Posicao %i",i);
         encontrei = 1;
        }
    }

       if  (encontrei == 0)
       {
           printf("Valor nao encontrado");
       }


        return 0;
    }
