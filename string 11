#include <stdio.h>
#include <string.h>

int main()
{
   char v[20],nv[20];
   int valor,i,cont=0;

   scanf("%s",v);
   valor=strlen(v);


       for(i=(valor-1);i>=0;i--)
       {
            nv[cont] = v[i];
            cont++;

       }

       nv[cont] = 0;



       if(strcmp(nv,v) == 0)
       {
           printf("sim\n");
       }
       else
       {
           printf("nao\n");
       }



return 0;
}
