#include <stdio.h>

int main()
{
     int m[4][5], v[10]= {0,1,2,3,4,5,7,8,9};
     int i,j,num=0,cont[10]={0,0,0,0,0,0,0,0,0};

        printf("Digite os valores da matriz: \n");

        for(i=0;i<4;i++)
        {
        for(j=0;j<5;j++)
           {
        scanf("%i", &m[i][j]);
           }
        printf("\n");
        }

            for(i=0;i<4;i++)
            {
            for(j=0;j<5;j++)
               {

                cont[m[i][j]]++;

               }

            }
            for(i=0;i<10;i++)
            {
            printf("%i aparece %i vezes em A\n",i,cont[i]);
            }
            

     return 0;
}
