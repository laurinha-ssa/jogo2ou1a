#include <stdio.h>
#include <math.h>

int main()
{
  int c1,q1;
  float v1;

    scanf("%i %i",&c1,&q1);
    //cada condicional Ã© para um codigo de produto diferente
    if(c1==1)
    {
        //valor de codigo * quantidade
        v1=4.00*q1;
        printf("Total: R$ %.2f\n",v1);
    }
    else if(c1==2)
    {
        //valor de codigo * quantidade
        v1=4.50*q1;
        printf("Total: R$ %.2f\n",v1);
    }
    else if(c1==3)
    {
        //valor de codigo * quantidade
        v1=5.00*q1;
        printf("Total: R$ %.2f\n",v1);
    }
    else if(c1==4)
    {
        //valor de codigo * quantidade
        v1=2.00*q1;
        printf("Total: R$ %.2f\n",v1);
    }
    else if(c1==5)
    {
        //valor de codigo * quantidade
        v1=1.50*q1;
        printf("Total: R$ %.2f\n",v1);
    }
    return 0;
}
